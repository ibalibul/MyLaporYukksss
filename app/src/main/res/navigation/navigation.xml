<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/spalashScreenFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.mylaporyukksss.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_informasiIuranFragment"
            app:destination="@id/informasiIuranFragment" />
        <action
            android:id="@+id/action_homeFragment_to_pengaduanMasyarakatFragment"
            app:destination="@id/pengaduanMasyarakatFragment" />
        <action
            android:id="@+id/action_homeFragment_to_detailPengaduanFragment"
            app:destination="@id/detailPengaduanFragment" />
        <action
            android:id="@+id/action_homeFragment_to_statistikDesaFragment"
            app:destination="@id/statistikDesaFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.mylaporyukksss.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.mylaporyukksss.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/spalashScreenFragment"
        android:name="com.example.mylaporyukksss.SpalashScreenFragment"
        android:label="fragment_spalash_screen"
        tools:layout="@layout/fragment_spalash_screen" >
        <action
            android:id="@+id/action_spalashScreenFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailPengaduanFragment"
        android:name="com.example.mylaporyukksss.DetailPengaduanFragment"
        android:label="fragment_detail_pengaduan"
        tools:layout="@layout/fragment_detail_pengaduan" >
        <action
            android:id="@+id/action_detailPengaduanFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/informasiIuranFragment"
        android:name="com.example.mylaporyukksss.InformasiIuranFragment"
        android:label="fragment_informasi_iuran"
        tools:layout="@layout/fragment_informasi_iuran" >
        <action
            android:id="@+id/action_informasiIuranFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_informasiIuranFragment_to_detailIuranFragment"
            app:destination="@id/detailIuranFragment" />
    </fragment>
    <fragment
        android:id="@+id/pengaduanMasyarakatFragment"
        android:name="com.example.mylaporyukksss.PengaduanMasyarakatFragment"
        android:label="fragment_pengaduan_masyarakat"
        tools:layout="@layout/fragment_pengaduan_masyarakat" >
        <action
            android:id="@+id/action_pengaduanMasyarakatFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.mylaporyukksss.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_profileFragment_to_helpFragment"
            app:destination="@id/helpFragment" />
        <action
            android:id="@+id/action_profileFragment_to_detailAccountFragment"
            app:destination="@id/detailAccountFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailIuranFragment"
        android:name="com.example.mylaporyukksss.DetailIuranFragment"
        android:label="fragment_detail_iuran"
        tools:layout="@layout/fragment_detail_iuran" >
        <action
            android:id="@+id/action_detailIuranFragment_to_informasiIuranFragment"
            app:destination="@id/informasiIuranFragment" />
    </fragment>
    <fragment
        android:id="@+id/statistikDesaFragment"
        android:name="com.example.mylaporyukksss.StatistikDesaFragment"
        android:label="fragment_statistik_desa"
        tools:layout="@layout/fragment_statistik_desa" >
        <action
            android:id="@+id/action_statistikDesaFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_statistikDesaFragment_to_detailPotensiFragment"
            app:destination="@id/detailPotensiFragment" />
    </fragment>
    <fragment
        android:id="@+id/helpFragment"
        android:name="com.example.mylaporyukksss.HelpFragment"
        android:label="fragment_help"
        tools:layout="@layout/fragment_help" >
        <action
            android:id="@+id/action_helpFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailPotensiFragment"
        android:name="com.example.mylaporyukksss.DetailPotensiFragment"
        android:label="fragment_detail_potensi"
        tools:layout="@layout/fragment_detail_potensi" >
        <action
            android:id="@+id/action_detailPotensiFragment_to_statistikDesaFragment"
            app:destination="@id/statistikDesaFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailAccountFragment"
        android:name="com.example.mylaporyukksss.DetailAccountFragment"
        android:label="fragment_detail_account"
        tools:layout="@layout/fragment_detail_account" >
        <action
            android:id="@+id/action_detailAccountFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
</navigation>